{
   "name": "Chrome React Authorization Extension.",
   "description": "Authorization with password.",
   "version": "1.0",
   "manifest_version": 3,
   "permissions": ["storage"],
   "host_permissions": ["http://localhost:3000/"],
   "action": {
       "default_popup": "index.html",
       "default_title": "Chrome Authorization App."
   },
   "icons": {
       "16": "auth16.png",
       "48": "auth48.png",
       "128": "auth128.png"
   },
   "content_security_policy": {
        "extension_page":"script-src 'self' 'wasm-unsafe-eval'; object-src 'self'" 
    }
}
